{
    "name": "devart/nzimmr",
    "type": "silverstripe-recipe",
    "description": "Website",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-cms": "4.*",
        "undefinedoffset/sortablegridfield": "2.*",
        "bummzack/sortablefile": "2.*",
        "symbiote/silverstripe-gridfieldextensions": "^3.1",
        "wilr/silverstripe-googlesitemaps": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Devart\\": "app/src/"
        },
        "classmap": [
            "app/src/Pages/Page.php",
            "app/src/Pages/PageController.php"
        ]
    },
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    }
}
